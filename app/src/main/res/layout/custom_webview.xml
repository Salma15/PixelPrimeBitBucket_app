<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/share_container_layout_id"
    android:orientation="vertical">

    <com.medisensehealth.fdccontributor.views.CustomTextViewBold
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/share_emr"
        android:visibility="gone"
        android:layout_gravity="right"
        android:background="@color/light_bg7"
        android:textColor="@color/colorPrimary"
        android:layout_marginRight="@dimen/dp_10"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        android:paddingTop="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_10"
        android:text="Share EMR"/>

    <WebView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/webview"
        android:minHeight="@dimen/dp_400"/>
</LinearLayout>
